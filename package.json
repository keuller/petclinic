{
    "name": "petclinic",
    "version": "1.0.0",
    "description": "PetClinic Monilitico",
    "main": "index.js",
    "private": true,
    "type": "module",
    "scripts": {
        "clean:apps": "pnpm --filter './apps/*' clean",
        "clean:mods": "pnpm --filter './packages/*' clean",
        "dev": "pnpm --filter './apps/*' dev",
        "build:apps": "pnpm --filter './apps/*' build",
        "build:mods": "pnpm --filter './packages/*' build"
    },
    "author": "Keuller Magalhaes",
    "license": "ISC",
    "engines": {
        "node": "<=18.X"
    },
    "packageManager": "pnpm@8.7.5",
    "workspaces": [
        "apps/*",
        "packages/*"
    ],
    "devDependencies": {
        "rimraf": "^5.0.1",
        "typescript": "^5.2.2",
        "vite": "^4.4.9"
    }
}
